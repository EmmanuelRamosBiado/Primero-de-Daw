import java.util.Iterator;

public class Kata {
  
    public static int speedLimit(final int speed, final int[] signals) {
      int multa = 0;
      
      for (int i = 0; i < signals.length; i++) {
		if(signals[i]-speed>=10 || signals[i]-speed<= 19) {
			multa += 100;
		}else if(signals[i]-speed>=20 || signals[i]-speed<= 29){
			multa += 250;
		} else if(signals[i]-speed>=30) {
			multa += 500;
		}
	}
      
      return multa;
    }
}